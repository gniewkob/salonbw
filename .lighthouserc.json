{
  "ci": {
    "collect": {
      "url": [
        "https://salon-bw.pl/",
        "https://salon-bw.pl/services",
        "https://salon-bw.pl/services/coloring",
        "https://salon-bw.pl/services/highlights",
        "https://salon-bw.pl/services/balayage",
        "https://salon-bw.pl/appointments",
        "https://dev.salon-bw.pl/"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.8 }],
        "categories:accessibility": ["warn", { "minScore": 0.9 }],
        "categories:seo": ["warn", { "minScore": 0.85 }],
        "first-contentful-paint": ["warn", { "maxNumericValue": 4000, "aggregationMethod": "median" }],
        "largest-contentful-paint": ["warn", { "maxNumericValue": 4000, "aggregationMethod": "median" }],
        "total-blocking-time": ["warn", { "maxNumericValue": 300, "aggregationMethod": "median" }],
        "cumulative-layout-shift": ["warn", { "maxNumericValue": 0.1, "aggregationMethod": "median" }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
