strict-peer-dependencies=true
#//registry.npmjs.org/:_authToken=${NPM_TOKEN}
registry=https://registry.npmjs.org/
# pnpm v10 on Linux (GitHub Actions) fails to hardlink nested dist/ directories
# from the content store into the virtual store (observed with @next/env@15.5.10).
# copy mode avoids hardlinks and ensures all files are present after install.
package-import-method=copy
