    "/logs": {
      "get": {
        "operationId": "LogsController_getLogs",
        "parameters": [
          {
            "name": "userId",
            "required": false,
            "in": "query",
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "action",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string",
              "enum": [
                "USER_LOGIN",
                "LOGIN_FAIL",
                "USER_REGISTERED",
                "AUTHORIZATION_FAILURE",
                "PRODUCT_CREATED",
                "PRODUCT_UPDATED",
                "PRODUCT_DELETED",
                "SERVICE_CREATED",
                "SERVICE_UPDATED",
                "SERVICE_DELETED",
                "APPOINTMENT_CREATED",
                "APPOINTMENT_CANCELLED",
                "APPOINTMENT_COMPLETED",
                "APPOINTMENT_RESCHEDULED",
                "COMMISSION_CREATED"
              ]
            }
          },
          {
            "name": "from",
            "required": false,
            "in": "query",
            "schema": {
              "format": "date-time",
              "type": "string"
            }
          },
          {
            "name": "to",
            "required": false,
            "in": "query",
            "schema": {
              "format": "date-time",
              "type": "string"
            }
          },
          {
            "name": "page",
            "required": false,
            "in": "query",
            "schema": {
              "default": 1,
              "type": "number"
            }
          },
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "schema": {
              "default": 10,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of logs"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Get logs",
        "tags": [
          "logs"
        ]
      }
    },
