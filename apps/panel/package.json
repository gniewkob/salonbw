{
    "name": "@salonbw/panel",
    "version": "0.1.0",
    "private": true,
    "license": "MIT",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "dev": "node scripts/start-dev-server.js",
        "dev:raw": "next dev",
        "build": "next build && node scripts/generate-next-data.mjs",
        "prebuild": "node scripts/ensure-local-deps.js",
        "analyze": "ANALYZE=true next build",
        "start": "next start",
        "start:standalone": "node app.js",
        "lint": "eslint src",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write '**/*.{ts,tsx,js,jsx,css,md}'",
        "test": "jest",
        "test:visual": "playwright test tests/visual/versum-admin.spec.ts",
        "cypress:install": "cypress install",
        "start:e2e": "pnpm run start:standalone",
        "e2e": "node scripts/server-utils.js cleanup && NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_URL:-/api} pnpm run build && NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_URL:-/api} start-server-and-test \"pnpm run start:e2e\" http://localhost:3000 'cypress run'",
        "e2e:ci": "NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_URL:-/api} pnpm run build && NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_URL:-/api} start-server-and-test \"pnpm run start:e2e\" http://localhost:3000 'cypress run'",
        "e2e:chrome": "node scripts/server-utils.js cleanup && NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_URL:-/api} pnpm run build && NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_URL:-/api} start-server-and-test \"pnpm run start:e2e\" http://localhost:3000 'cypress run --browser chrome:stable'",
        "e2e:chrome:split": "cypress run --browser chrome:stable",
        "e2e:chrome:node": "node scripts/run-cypress.js",
        "ports:cleanup": "node scripts/server-utils.js cleanup",
        "ports:check": "node scripts/server-utils.js find-port",
        "build:production": "NODE_ENV=production next build",
        "prepare": "husky install",
        "bundle:check": "node scripts/check-bundle-size.mjs"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@fullcalendar/core": "^6.1.20",
        "@fullcalendar/daygrid": "^6.1.18",
        "@fullcalendar/interaction": "^6.1.18",
        "@fullcalendar/react": "^6.1.18",
        "@fullcalendar/timegrid": "^6.1.18",
        "@heroicons/react": "^2.2.0",
        "@next/bundle-analyzer": "^16.0.1",
        "@next/env": "15.5.10",
        "@salonbw/api": "workspace:*",
        "@sentry/nextjs": "^10.35.0",
        "@swc/helpers": "^0.5.19",
        "@tanstack/react-query": "^5.90.7",
        "autoprefixer": "^10.4.22",
        "date-fns": "^4.1.0",
        "js-cookie": "^3.0.5",
        "next": "15.5.10",
        "picocolors": "^1.1.0",
        "react": "18.3.1",
        "react-datepicker": "^7.6.0",
        "react-dom": "18.3.1",
        "react-hot-toast": "^2.4.0",
        "recharts": "^3.7.0",
        "sharp": "^0.34.5",
        "styled-jsx": "^5.1.7"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@jest/globals": "^30.0.1",
        "@playwright/test": "^1.58.2",
        "@suchipi/cypress-plugin-snapshots": "^1.4.5",
        "@tailwindcss/postcss": "^4",
        "@testing-library/jest-dom": "^6",
        "@testing-library/react": "^16.1.0",
        "@types/jest": "^30",
        "@types/js-cookie": "^3.0.6",
        "@types/node": "^24",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "cypress": "^15.7.1",
        "eslint": "^8",
        "eslint-config-next": "15.5.10",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.5.1",
        "husky": "^9.0.11",
        "jest": "^30.0.1",
        "jest-environment-jsdom": "^30.0.4",
        "pixelmatch": "^7.1.0",
        "pngjs": "^7.0.0",
        "prettier": "^3.4.2",
        "start-server-and-test": "^2.0.1",
        "tailwindcss": "^4",
        "ts-jest": "^29",
        "typescript": "^5",
        "typescript-eslint": "^8.56.0",
        "web-streams-polyfill": "^4.1.0",
        "whatwg-fetch": "^3.6.20"
    }
}
