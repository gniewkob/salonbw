{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "node scripts/start-dev-server.js",
    "dev:raw": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:production": "node scripts/start-production.js",
    "lint": "eslint src",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,css,md}'",
    "test": "jest",
    "cypress:install": "cypress install",
    "e2e": "node scripts/server-utils.js cleanup && NEXT_PUBLIC_API_URL=/api npm run build && NEXT_PUBLIC_API_URL=/api start-server-and-test start http://localhost:3000 'cypress run'",
    "e2e:ci": "NEXT_PUBLIC_API_URL=/api npm run build && NEXT_PUBLIC_API_URL=/api start-server-and-test start http://localhost:3000 'cypress run'",
    "ports:cleanup": "node scripts/server-utils.js cleanup",
    "ports:check": "node scripts/server-utils.js find-port",
    "build:production": "NODE_ENV=production next build"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@next/bundle-analyzer": "^14.2.32",
    "@radix-ui/react-select": "^2.2.6",
    "@sentry/nextjs": "^8.55.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "formik": "^2.4.6",
    "next": "14.2.31",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "^2.4.0",
    "sharp": "^0.34.3",
    "yup": "^1.7.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^30",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^14.5.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.31",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.1",
    "jest-environment-jsdom": "^30.0.4",
    "msw": "^2",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^4",
    "ts-jest": "^29",
    "typescript": "^5",
    "typescript-eslint": "^8.37.0",
    "web-streams-polyfill": "^4.1.0",
    "whatwg-fetch": "^3.6.20"
  }
}
